// Example demonstrating struct field access using dot operator

struct Person {
    name: string;
    age: i32;
}

fn main() {
    let person = new Person {
        name: 'Alice',
        age: 30
    };
    
    // Access struct fields using dot operator
    let person_name = person.name;
    let person_age = person.age;

    person.name = 'Bob';
    person.age = 15;
    
    return person_name;
}

print(main(), '\n');
let mut x = 50;
let mut y = 10;

let mut z = 0;

// swap x and y if x > y
if x > y {
    let tmp = x;
    x = y;
    y = tmp;
} else {
    z = 1;
}

// Test ==, !=, >=, <= operators
let eq = (x == y);
let ne = (x != y);
let ge = (x >= y);
let le = (x <= y);

if x == 50 {
    z = z + 10;
}
if y != 10 {
    z = z + 100;
}
if x >= y {
    z = z + 1000;
}
if y <= x {
    z = z + 10000;
}

